<?xml version="1.0"?>
<extension key="com.octopus8.contridivide" type="module">
  <file>contridivide</file>
  <name>Contridivide</name>
  <description>An extension that creates seperate identifiers for nontax, taxable and donation in-kind contributions.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Melissa, Shawn, MayPhyu</author>
    <email>melissa@octopus8.com / nadiya.m.madden@gmail.com</email>
  </maintainer>
  <urls>
    <url desc="Github Page">https://github.com/CloverForby/Contridivide.git</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2023-12-07</releaseDate>
  <version>1.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.66</ver>
  </compatibility>
  <comments>This is an updated extension which includes receipt id for donation in-kind and receipt id will be 6-chars long.</comments>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Contridivide</namespace>
    <format>23.02.1</format>
    <angularModule>crmContridivide</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
  <requires>
	<ext>civi_contribute</ext>
  </requires>
</extension>
